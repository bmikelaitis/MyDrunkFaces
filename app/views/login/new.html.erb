<%= render 'shared/pagetitle', title: 'Log In' %>
<div class="row">
	<div class="well col-xs-6 col-lg-6 col-lg-offset-3 col-xs-offset-3 center">
<%= form_tag 'login' do %>
<%= label_tag :email %>
<%= text_field_tag :email %>

<%= label_tag :password %>
<%= password_field_tag :password %>

<%= submit_tag 'Login', class: 'btn border-blue' %>
<% end %>
	</div>
</div>